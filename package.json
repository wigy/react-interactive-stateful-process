{
  "name": "react-interactive-stateful-process",
  "version": "0.1.13",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": "git@github.com:wigy/react-interactive-stateful-process.git",
  "author": "Tommi Ronkainen <tommi.ronkainen@gmail.com>",
  "license": "GPL-3.0-only",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "interactive-stateful-process": "^0.0.1"
  },
  "scripts": {
    "demo": "NODE_ENV=development webpack serve --host 0.0.0.0 --port ${PORT:=3301} --config ./config/webpack.dev.js",
    "dev": "tsc -w",
    "doc": "rm -fr doc/* && typedoc --out doc src/*.ts src/*/*.ts src/*/*/*.ts src/*.tsx src/*/*.tsx src/*/*/*.tsx",
    "doc:watch": "rm -fr doc/* && typedoc --out doc --watch src/*.ts src/*/*.ts src/*/*/*.ts src/*.tsx src/*/*.tsx src/*/*/*.tsx",
    "start": "npm run build",
    "build": "rm -rf dist/* && tsc && yarn run doc",
    "lint": "echo \"\\033[0;31mNo linting system for react interactive stateful process yet.\\033[0m\"",
    "test": "echo \"\\033[0;31mNo testing system for react interactive stateful process yet.\\033[0m\"",
    "test:ci": "yarn install && yarn run lint && yarn test"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.13.10",
    "@types/webpack-env": "^1.16.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.1.0",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.5",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.12.1",
    "mobx": "^6.3.0",
    "mobx-react": "^7.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.15"
  }
}
